{
  "name": "marishop/shoppingcart",
  "description": "Laravel Shoppingcart",
  "keywords": [
    "laravel",
    "shoppingcart"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad Syafiq",
      "email": "syafiiqaufa@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11",
    "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11",
    "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0 || ~10.0 || ^10.5",
    "mockery/mockery": "~1.6",
    "orchestra/testbench": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "Marishop\\Shoppingcart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marishop\\Tests\\Shoppingcart\\": "tests/"
    }
  },
  "suggest": {
    "Marishop/notify": "Simple flash notifications for Laravel"
  },
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/svmmerse/cartshopping"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Marishop\\Shoppingcart\\ShoppingcartServiceProvider"
      ],
      "aliases": {
        "Cart": "Marishop\\Shoppingcart\\Facades\\Cart"
      }
    }
  }
}
